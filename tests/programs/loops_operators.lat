int main() {
    int i = 0;
    int s = 0;

    while (i < 10) {
        s = s + i;
        i = i + 1;
    }
    printInt(s);

    i = 0;
    s = 0;
    while (10 > i) {
        s = s + i;
        i = i + 1;
    }
    printInt(s);

    i = 0;
    s = 0;
    while (i <= 9) {
        s = s + i;
        i = i + 1;
    }
    printInt(s);

    i = 0;
    s = 0;
    while (9 >= i) {
        s = s + i;
        i = i + 1;
    }
    printInt(s);

    if (1 == 2) {
        printInt(0);
    } else {
        printInt(1);
    }
    if (1 != 1) {
        printInt(0);
    } else {
        printInt(1);
    }

    return 0;


}
